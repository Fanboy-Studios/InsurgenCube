RewriteEngine On
RewriteRule ^cube index.php?cube [L]
RewriteRule ^xml index.php?xml [L]
RewriteRule ^reg(?:ister)?/([0-5]{4,5})/([0-9]{1,5})/?$ index.php?register&port=$2&proto=$1 [L]
RewriteRule ^auth/([0-9]{1,9})/([0-9a-fA-F]{40})/? index.php?authchal&id=$1&ans=$2 [L]
RewriteRule ^auth/([0-9]{1,9})/? index.php?authreq&id=$1 [L]